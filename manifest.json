{
"name": "Q工具集",
"version": "1.0",
"manifest_version":2,
"description": "chrome的工具集合",
"offline_enabled":false,
"icons": { "16": "images/icon.png",
           "48": "images/icon.png",
          "128": "images/icon.png"
          },
 "background": { "scripts": ["scripts/background.js"] },
"browser_action": {
          "default_icon": {                    // 可选
            "19": "images/icon.png",           // 可选
            "38": "images/icon.png"            // 可选
          },
          "default_title": "Q工具集",      // 可选，在工具提示中显示
          "default_popup": "html/popup.html",        // 可选
          "null":null
},
"permissions": [
	"<all_urls>",
	"bookmarks",
	"unlimitedStorage",
  "webRequest",
  "webRequestBlocking",
  "proxy", 
  "tabs", 
  "browsingData", 
  "webRequestBlocking", 
  "http://*/*", 
  "https://*/*"
	]
}